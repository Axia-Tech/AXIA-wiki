{
  "scripts": {
    "axialunar:clean": "shx rm -rf node_modules/.cache; docusaurus clear axialunar-guide",
    "axialunar:start": "docusaurus start axialunar-guide --port 3402",
    "preaxialunar:build": "npm-run-all -ln axialunar:clean -p \"fetch --isAXIASolar=false\" mirror axialunar:copy-assets --aggregate-output",
    "axialunar:build": "cross-env BUILDING=true docusaurus build axialunar-guide; node axialunar-guide/postprocess",
    "axialunar:inject": "node --experimental-json-modules scripts/inject.mjs --rootDir axialunar-guide/build/axialunar-guide-hosting",
    "axialunar:publish-gh-pages": "cross-env PUBLISHING=true docusaurus deploy axialunar-guide",
    "axialunar:copy-assets": "cp -R docs/assets axialunar-guide/i18n/zh-CN/docusaurus-plugin-content-docs/current",
    "axialunar:i18n:download": "yarn axiasolar:crowdin:download:zh-CN",
    "axialunar:i18n:copy-translations": "cp -R axiasolar-wiki/i18n/zh-CN/docusaurus-plugin-content-docs/current axialunar-guide/i18n/zh-CN/docusaurus-plugin-content-docs/current",
    "axialunar:pdf": "yarn pdf --initialDocURLs \"http://localhost:3000/docs/axialunar-index\" --outputPDFFilename=\"axialunar-guide/static/axialunar-wiki.pdf\" --coverTitle=\"AXIALUNAR GUIDE\"",
    "axiasolar:clean": "shx rm -rf node_modules/.cache; docusaurus clear axiasolar-wiki",
    "axiasolar:start": "yarn generateRegistryTable; docusaurus start axiasolar-wiki --port 3401",
    "preaxiasolar:build": "npm-run-all -ln axiasolar:clean -p fetch generateRegistryTable mirror axiasolar:copy-assets:* --aggregate-output",
    "axiasolar:build": "cross-env BUILDING=true docusaurus build axiasolar-wiki; node axiasolar-wiki/postprocess",
    "axiasolar:inject": "node --experimental-json-modules scripts/inject.mjs --rootDir axiasolar-wiki/build/axiasolar-wiki --node wss://rpc.axiasolar.io",
    "axiasolar:publish-gh-pages": "cross-env PUBLISHING=true docusaurus deploy axiasolar-wiki",
    "axiasolar:pdf": "yarn pdf --initialDocURLs \"http://localhost:3000/docs/getting-started\" --outputPDFFilename=\"axiasolar-wiki/static/axiasolar-wiki.pdf\" --coverTitle=\"AXIASOLAR WIKI\"",
    "copy:translations": "cp -R axiasolar-wiki/translated_docs axialunar-guide ",
    "publish-gh-pages": "cd axiasolar-wiki; docusaurus-publish",
    "write-translations": "docusaurus-write-translations",
    "version": "docusaurus-version",
    "rename-version": "docusaurus-rename-version",
    "crowdin-upload": "crowdin --config ../crowdin-docs-v1.yaml upload sources --auto-update -b master",
    "crowdin-download": "crowdin --config ../crowdin-docs-v1.yaml download -b master",
    "generateRegistryTable": "node --experimental-json-modules scripts/generateRegistryTable.mjs > docs/build-ss58-registry.md",
    "generateErrorTable": "node --experimental-json-modules scripts/generateErrorTable.mjs >> docs/maintain-errors.md",
    "axiasolar:crowdin:upload:source": "crowdin upload sources --config ./crowdin.yaml -b master",
    "axiasolar:crowdin:download:ru": "crowdin download --config ./crowdin.yaml -b master -l ru",
    "axiasolar:crowdin:download:zh-CN": "crowdin download --config ./crowdin.yaml -b master -l zh-CN",
    "axiasolar:copy-assets:ru": "shx mkdir -p axiasolar-wiki/i18n/ru-RU/docusaurus-plugin-content-docs/current; shx cp -R docs/assets axiasolar-wiki/i18n/ru-RU/docusaurus-plugin-content-docs/current",
    "axiasolar:copy-assets:zh-CN": "shx mkdir -p axiasolar-wiki/i18n/zh-CN/docusaurus-plugin-content-docs/current; shx cp -R docs/assets axiasolar-wiki/i18n/zh-CN/docusaurus-plugin-content-docs/current",
    "pdf": "npx mr-pdf --contentSelector=\"article\" --paginationSelector=\".pagination-nav__item--next > a\" --pdfFormat=\"A4\" --pdfMargin=\"90,60,80,60\"",
    "fetch": "node --experimental-json-modules scripts/fetch.mjs --isAXIASolar",
    "mirror": "node --experimental-json-modules scripts/mirror.mjs",
    "crowdin": "crowdin",
    "docusaurus": "docusaurus",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@actions/core": "^1.5.0",
    "@actions/github": "^5.0.0",
    "@axiasolar-js/api": "^1.0.0",
    "@axiasolar-js/keyring": "^1.0.0",
    "@crowdin/cli": "^3.6.5",
    "@docusaurus/core": "^2.0.0-beta.6",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.6",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.6",
    "@docusaurus/preset-classic": "^2.0.0-beta.6",
    "@octokit/rest": "^18.10.0",
    "axios": "^0.21.4",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "fs-extra": "^10.0.0",
    "husky": "^7.0.2",
    "ipfs-http-client": "^52.0.2",
    "katex": "^0.13.18",
    "loud-rejection": "^2.2.0",
    "mr-pdf": "^1.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.3",
    "react-dom": "^17.0.2",
    "remarkable-katex": "^1.1.8",
    "shx": "^0.3.3",
    "unist-util-visit": "2.0.3",
    "yargs": "^17.2.0"
  },
  "dependencies": {
    "@axiasolar-js/util": "^1.0.0",
    "@axiasolar-js/wasm-crypto": "1.1.0",
    "@docusaurus/theme-live-codeblock": "^2.0.0-beta.6",
    "ansi-html": "^0.0.7",
    "immer": "^9.0.6",
    "remark-docusaurus-tabs": "^0.1.0",
    "replace-in-file": "^6.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}

